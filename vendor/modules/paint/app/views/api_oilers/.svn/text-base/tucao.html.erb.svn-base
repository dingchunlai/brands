    document.write('a.axx:active,a.axx:hover{text-decoration:none;color:#FF0000;border:#FF0000 1px solid;line-height:12px;font:12px sans-serif;background-color:#FFFF99;}');    
    document.write('.bxx{padding:1px;width:12px;height:13px;display:block;text-align:center;margin:5px;}');    
