      <script type="text/javascript">
        function ge(id) {
          return document.getElementById(id);
        }

        function trim(str) {
          return str.replace(/^\s*(.*?)[\s\n]*$/g, '$1');
        }

        function submitForm(form) {
          if (trim(ge("bm_name").value) == "") {
            alert("您必须填写【姓名】！")
            return false;
          }
          if (trim(ge("bm_phone").value) == "") {
            alert("您必须填写【电话】！")
            return false;
          }
          if (!trim(ge("bm_phone").value).match(/^\d{11}$/)) {
            alert("【电话】号码必须为11位数字！")
            return false;
          }
          if (trim(ge("bm_area").value) == "") {
            alert("您必须填写【装修面积】！")
            return false;
          }
          if (!trim(ge("bm_area").value).match(/^[1-9][0-9]*$/)) {
            alert("【装修面积】必须为大于0整数数字！")
            return false;
          }
          if (trim(ge("bm_room").value) == "") {
            alert("您必须选择【房型】！")
            return false;
          }
          if (trim(ge("bm_address").value) == "") {
            alert("您必须填写【楼盘名称】！")
            return false;
          }
          if (trim(ge("firm_city").value) == "") {
            alert("您必须选择装修区域【省份】！")
            return false;
          }
          if (trim(ge("firm_district").value) == "") {
            alert("您必须选择装修区域【城市】！")
            return false;
          }
          if (trim(ge("bm_budget").value) == "") {
            alert("您必须填写【装修预算】！")
            return false;
          }
          if (!trim(ge("bm_budget").value).match(/^[1-9][0-9]*$/))
          {
            if(ge("bm_budget").value <= 0 || ge("bm_budget").value >= 1000)
            {
              alert("【装修预算】必须是大于0小于1000的整数！")
              return false;            
            }
          }

          return true;
        }
      </script>

      <script type="text/javascript">
        $(".tanchuall_open").click(function() {
          $(".tanchuall_bg").show();
          $(".tanchuall").show();
        });
        $(".tanchuall_close").click(function() {
          $(".tanchuall_bg").hide();
          $(".tanchuall").hide();
        })

        $("#firm_city").change(function() {
          if ($("#firm_city").val().length == 0) {
            $("#firm_district").empty();
            $("<option value=''>市/地区</option>").appendTo("#firm_district");
          } else {
            $.ajax({
              type:'get',
              url:document.URL+'/get_cities',
              data:'provinces_id=' + $("#firm_city option:selected").attr('data_id'),
              dataType:'json',
              success:function(data){
                $("#firm_district").empty();
                $("<option value=''>市/地区</option>").appendTo("#firm_district");
                $.each(data, function(i) {
                  $("<option value='" + data[i][0] + "' data_id='" + data[i][1] + "'>" + data[i][0] + "</option>").appendTo("#firm_district");
                });
              }
            })
          }
        });

        $("#firm_district").change(function() {
          if ($("#firm_district").val().length == 0) {
            $("#firm_area").empty();
            $("<option value=''>县/市</option>").appendTo("#firm_area");
          } else {
            $.ajax({
              type:'get',
              url:document.URL+'/get_districts',
              data:'citys_id=' + $("#firm_district option:selected").attr('data_id'),
              dataType:'json',
              success:function(data){
                $("#firm_area").empty();
                $("<option value=''>县/市</option>").appendTo("#firm_area");
                $.each(data, function(i) {
                  $("<option value='" + data[i][0] + "' data_id='" + data[i][1] + "'>" + data[i][0] + "</option>").appendTo("#firm_area");
                });
              }
            })
          }
        });

      </script>
      <div class="tanchuall_bg" style="display:none;"></div>
    <% end  %>
  </body>
