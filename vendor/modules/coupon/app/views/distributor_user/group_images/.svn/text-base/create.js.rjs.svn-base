if @image.errors.count == 0
  page.insert_html :top, :img_list, :partial => 'distributor_user/group_images/image', :locals => { :image => @image, :img_type => @img_type, :default_img => @resource.default_image_id }
  page << "$($('#cancel_edit').parent()).html('');"
else
  page.replace_html :hy_upform_error_msg, @image.errors.full_messages.join("<br />")
end