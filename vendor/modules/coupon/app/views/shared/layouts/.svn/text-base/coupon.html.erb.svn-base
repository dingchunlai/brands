<% content_for :html_head do %>
  <link href="http://js.51hejia.net/css/daijq.css" rel="stylesheet" type="text/css" />
  <% if Rails.env == "production" %>
    <link href="http://js.51hejia.net/css/diyongquan.css" rel="stylesheet" type="text/css" />
