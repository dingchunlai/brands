  <script src="http://js2.51hejia.net/js/loopedslider.min.js" type=text/javascript></script>
  <script type="text/javascript" src="http://js2.51hejia.net/js/jquery/plugins/cookie/1.0.0/jquery.cookie.min.js"></script>
