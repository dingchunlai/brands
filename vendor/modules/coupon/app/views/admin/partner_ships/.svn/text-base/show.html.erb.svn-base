<%= top_bar :title       => '合作伙伴管理页',
            :button      => ['创建合作伙伴', new_admin_partner_ship_path],
            :breadcrumbs => [
                    ['首页', coupon_admin_index_path],
                    ['合作伙伴', admin_partner_ships_path]
            ]
%>
<div class="table">
  <img src="/stylesheets/pretty_admin/img/bg-th-left.gif" width="8" height="7" alt="" class="left"/>
  <img src="/stylesheets/pretty_admin/img/bg-th-right.gif" width="7" height="7" alt="" class="right"/>
  <table class="listing form" cellpadding="0" cellspacing="0">
    <tr>
      <th class="full" colspan="2">合作伙伴信息</th>
    </tr>
    <tr class="bg">
      <td class="first"><strong><%= PartnerShip.human_attribute_name("domain") %></strong></td>
      <td class="last"><%= @partner_ship.domain %></td>
    </tr>
    <tr>
      <td class="first"><strong><%= PartnerShip.human_attribute_name("code") %></strong></td>
      <td class="last"><%= @partner_ship.code %></td>
    </tr>
    <tr class="bg">
      <td class="first"><strong><%= PartnerShip.human_attribute_name("name") %></strong></td>
      <td class="last"><%= @partner_ship.name %></td>
    </tr>
    <tr>
      <td class="first"><strong><%= PartnerShip.human_attribute_name("summary") %></strong></td>
      <td class="last"><%= @partner_ship.summary %></td>
    </tr>
    <tr class="bg">
      <td class="first"><strong><%= PartnerShip.human_attribute_name("activity_begin_at") %></strong></td>
      <td class="last"><%= @partner_ship.activity_begin_at %></td>
    </tr>
    <tr>
      <td class="first"><strong><%= PartnerShip.human_attribute_name("activity_end_at") %></strong></td>
      <td class="last"><%= @partner_ship.activity_end_at %></td>
    </tr>

  </table>
</div>
