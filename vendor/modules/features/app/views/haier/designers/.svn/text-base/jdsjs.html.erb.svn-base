    function(){
      $(".haier_loginboxbg").hide();
      $(".haier_loginbox").hide();
    });
    $("#yuyue_form").submit(function(){
      var i = 0;
      var j = 0;
      if( $.trim($("#c1").val()).length == 0 || $.trim($("#c3").val()).length == 0 || $.trim($("#c4").val()).length == 0 || $.trim($("#c5").val()).length == 0 || $.trim($("#c6").val()).length == 0 || $.trim($("#c7").val()).length == 0 || $.trim($("#c8").val()).length == 0 || $.trim($("#c11").val()).length == 0 ){
        alert("所有信息都为必填项,请完整的填写信息");
        return false;
      }
      $("input[name*='c9[]']").each(function(){
        if($(this).attr("checked") == true){
          i = i + 1;
        }
      });
      $("input[name*='c12[]']").each(function(){
        if($(this).attr("checked") == true){
          j = j + 1;
        }
      });
      if(i == 0 || j == 0){
        alert("所有信息都为必填项,请完整的填写信息");
        return false;
      }
    });
  });
</script>
