<script type="text/javascript">
  $(".tanchuall_open").click(function() {
    $(".tanchuall_bg").show();
    $(".tanchuall").show();
  });
  $(".tanchuall_close").click(function() {
    $(".tanchuall_bg").hide();
    $(".tanchuall").hide();
  })

  $("#firm_city").change(function() {
    if ($("#firm_city").val().length == 0) {
      $("#firm_district").empty();
      $("<option value=''>市/地区</option>").appendTo("#firm_district");
    } else {
      $.ajax({
        type:'get',
        url:document.URL+'get_cities',
        data:'provinces_id=' + $("#firm_city option:selected").attr('data_id'),
        dataType:'json',
        success:function(data){
          $("#firm_district").empty();
          $("<option value=''>市/地区</option>").appendTo("#firm_district");
          $.each(data, function(i) {
            $("<option value='" + data[i][0] + "' data_id='" + data[i][1] + "'>" + data[i][0] + "</option>").appendTo("#firm_district");
          });
        }
      })
    }
  });

  $("#firm_district").change(function() {
    if ($("#firm_district").val().length == 0) {
      $("#firm_area").empty();
      $("<option value=''>县/市</option>").appendTo("#firm_area");
    } else {
      $.ajax({
        type:'get',
        url:document.URL+'get_districts',
        data:'citys_id=' + $("#firm_district option:selected").attr('data_id'),
        dataType:'json',
        success:function(data){
          $("#firm_area").empty();
          $("<option value=''>县/市</option>").appendTo("#firm_area");
          $.each(data, function(i) {
            $("<option value='" + data[i][0] + "' data_id='" + data[i][1] + "'>" + data[i][0] + "</option>").appendTo("#firm_area");
          });
        }
      })
    }
  });

</script>

