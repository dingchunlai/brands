<% dp_js 'dianping/yuyue.js'%>
<%
  top_applicants = applicant_top_firm(@user_city_code.to_i)
%>	
<div class="zxdp2010_cur margin10">
  <h3>本月预约排行榜</h3>
  <div class="zxdp2010_txt zxdp2010_kbtop right_applicant_2011 cur_height">
    <ul>
      <% top_applicants.each do |applicant|%>
        <li>
          <span class="zxdp2010_kbspan21"><a href="<%= firm_url(:id => applicant.deco_firm.id,:trailing_slash => "/")%>" target="_blank"><%= applicant.deco_firm.name_abbr %></a></span>
          <span class="zxdp2010_kbspan22"><%= applicant.count %>单</span>
          <span class="zxdp2010_kbspan18">
            <%= link_to "我要预约",new_dianping_applicant_path(:deco_firm_id => applicant.deco_firm.id), :title =>"免费预约量房",:id=>"applicant" ,:class => "applicant" %>
          </span>
        </li>
      <% end %>
    </ul>
  </div>
</div>
