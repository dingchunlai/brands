<% default_impression_text = '不多于5个字' %>
<%# 样式添加在页头 %>
<% content_for :html_head do %>
  <style type="text/css">
    .post_comment_table {
      border: 1px solid #D4D4D4;
      width: 450px;
      heigth: 300px;
    }
    .post_comment_table th {
      color: #044FB3;
      text-align: center;
      font-size: 14px;
      letter-spacing: 3px;
      width: 50px;
      padding: 5px 0;
    }
    .post_comment_table td {
      color: #8A8A8A;
      font-size: 12px;
      line-height: 30px;
      padding: 5px 0 5px 15px;
    }
  </style>
  <%=
  stylesheet_link_tag(
    jquery_resource(:plugin => 'tag_cloud', :version => '1.0.0', :resource => :css),
    jquery_resource(:plugin => 'jgrowl', :version => '1.2.4', :resource => :css)
  )
%>
<% end %>

<%# js添加到页脚 %>
<% content_for :html_body_end do %>
  <%=
  javascript_include_tag(
    jquery_resource(:plugin => 'tag_cloud', :version => '1.0.0'),
    jquery_resource(:plugin => 'jgrowl', :version => '1.2.4')
  )
%>
  <script type="text/javascript">
    function login_callback(arg) {
      if(arg == 'error') {
        alert('帐号有误，请重新输入。');
      } else {
        $('#comment_btn').colorbox({'open': true});
      }
    }

    function register_callback(arg) {
      if(arg) {
        if(typeof(arg) == 'object') {
          if(arg['error'] == 'code')
            alert('验证码错误。');
          else
            //alert('注册信息有误，请检查。');
              alert(arg['error']);
        } else {
          document.cookie = "ind_id" + "="+ arg + ";path=/;domain=.51hejia.com";
          $('#comment_btn').colorbox({'open': true});
        }
      } else {
        alert('很抱歉，发生未知错误，请稍后重试。');
      }
    }

    $(function(){
      $('.colorbox').colorbox();

      // 添加印象
      function addImpression(impression) {
        $.ajax({
          url: $('#impression_form').attr('action'),
          type: 'POST',
          data: $.param({title: impression}),
          dataType: 'json',

          error: function(request) {
            var msg;
            try {
              msg = eval('(' + request.responseText + ')').join('<br/>');
            } catch(e) {
              msg = '未知错误，请稍后再尝试。';
            }
            $.jGrowl(msg, {header: '出错啦'});
          },

          success: function(data) {
            $("#new_impression").text(data.join(" "));
            $.jGrowl('印象添加成功', {header: '感谢您的参与'});
          }
        });
      }

      $('#impression_form').submit(function(event) {
        event.preventDefault();
        var textField = $(this).find(':text'),
        impression = $.trim(textField.val());
        textField.val('<%= default_impression_text %>');
        if (impression != '' && impression != '<%= default_impression_text %>') addImpression(impression);
        return false;
      }).find(':text').
        focus(function() { if( $.trim($(this).val()) == '<%= default_impression_text %>' ) $(this).val('') }).
        blur(function() { if( $.trim($(this).val()) == '' ) $(this).val('<%= default_impression_text %>') });

      $('.impression').click(function(event) {
        addImpression($(this).text());
        event.preventDefault();
        // return false;
      });

      // 点评
      $('#comment_form').live('submit', function(event) {
        var submitBtn = $(this).find(':submit');
        submitBtn.attr('disabled', 'disabled');
        $.ajax({
          url: '<%=ajax_post_path(@brand.permalink, "comment")%>',
          type: 'POST',
          data: $(this).serialize(),
          dataType: 'json',

          error: function(request) {
            var msg = "由于以下错误，您的评论未能保存：\n  "
            try {
              msg +=
                $.map(eval('(' + request.responseText + ')'), function(error) {
                return error[1];
              }).join("\n  ");
            } catch(e) {
              msg += "未知错误"
            }

            alert(msg);
            submitBtn.attr('disabled', null);
          },

          success: function(data) {
            //alert(data.title);
            alert('点评添加成功！');
            submitBtn.attr('disabled', null);
            $.fn.colorbox.close();
          }
        });
        event.preventDefault();
        return false;
      });

      //
      $('#pop_login_form').live('submit', function(event) {
        if ($.trim($('#pop_user_name').val())==''){
          alert('请填写用户名！');
        }
        if ($.trim($('#pop_password').val())==''){
          alert('请填写登录密码！');
        }
        event.preventDefault();
        $.ajax({
          dataType: 'jsonp',
          jsonpCallback: 'login_callback',
          url: $(this).attr('action'),
          data: $(this).serialize(),
          error: function() {
            alert('很抱歉，发生未知错误，请稍后重试。')
          }
        });
      });

      //
      $('#signup_form').live('submit', function(event) {
        event.preventDefault();
        $.ajax({
          dataType: 'jsonp',
          jsonpCallback: 'register_callback',
          url: $(this).attr('action'),
          data: $(this).serialize(),
          error: function() {
            alert('您填写的注册信息有误，请更正后重试。')
          }
        });
      });

      // 印象标签
      $('.tag_cloud').tagCloud();
    });
  </script>
<% end %>

