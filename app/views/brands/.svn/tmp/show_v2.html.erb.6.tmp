              function(){
                fbauto();
              },2000);
            });

            fbstage.mouseover(function(){
              clearInterval(fbtimer);
            });

            fbstage.mouseout(function(){
              fbtimer = setInterval(
              function(){
                fbauto();
              },2000);
            });

            var fbslider = function (obj, attr){
              dis = attr * -fbdis;
              obj.css({left: dis},"slow");
            }

            var fbauto = function(){
              if( target < total){
                target++;
              }
              else{
                target = 0;
              }
              fbnav.eq(target).addClass("focus").siblings().removeClass("focus");
              fbslider(fbstage,target);
            }

            var fbtimer = setInterval(
            function(){
              fbauto();
            },2000);
          }
          //调用
          fbwrap($("#lb_num1 li"), $("#b_pln_wrap1"), 658);
          fbwrap($("#lb_num2 li"), $("#b_pln_wrap2"), 398);
        </script>
      </div>
    <% else %>
      <div class="b_pln_ljlbhot f_l">
        <% bbs_topic = bbs_topics[0] %>
        <% unless bbs_topic.blank? %>
          <h4><%=link_to bbs_topic.title, bbs_topic.url, :target=>'_blank'%></h4>
          <div class="b_pln_ljlbhotimg">
            <img src="<%=bbs_topic.image%>" alt="<%=bbs_topic.title%>" />
          </div>
          <p>
            <%=ul(bbs_topic.content, 190)%>
            <%=link_to '[详情]', bbs_topic.url, :target=>'_blank'%>
          </p>
        <% end %>
      </div>
    <% end %>
    <div class="b_pln_ljlb f_r">
      <ul>
        <% unless bbs_topics.nil? %>
          <% if bbs_topics.size > 1 %>
            <% bbs_topics[1,8].each do |topic| %>
              <li><%= link_to topic.title, topic.url, :target => '_blank' %></li>
            <% end %>
          <% end %>
        <% end %>
      </ul>
    </div>
  </div>
</div>

