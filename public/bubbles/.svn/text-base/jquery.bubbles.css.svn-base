/*-- 泡泡 --*/
.bubbles{overflow:hidden;position:relative;}
.bubbles a{ blr:expression(this.onFocus=this.blur()); /*    IE Opera    */ outline:none;   /*    FF Opera    */display:block; background:url(jquery.bubbles_bg.gif) no-repeat 0 0; color:#666; font-weight:bold; text-align:center;line-height:120%;height:100%;}
.bubbles a:hover{ text-decoration:none;outline:none;/*  FF Opera  */ cursor:pointer;}
.bubbles a span{ font-size:12px; font-weight:400; display:block; padding:28% 5px 5px; line-height:14px; height:70%;}
.bubbles a:link{color:#164A84;}
.bubbles div{ position:absolute; float:left; overflow:hidden;}
/*--橙黄色球--*/

