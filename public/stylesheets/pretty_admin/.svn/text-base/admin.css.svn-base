/* flash notice  */
.flash{
  border-width:1px;
  border-style:solid;
  margin:5px 0;padding:3px 0;
  text-indent:1em;
  width:100%;
}
.error{
  border-color:red;
  background-color:#FFD3D0;
}
.notice{
  border-color:green;
  background-color:#DAFFD1;
}

/*分页*/
.digg_pagination {
  background: white;
  margin: 15px;
  text-align: right;

  /* self-clearing method: */ }
  .digg_pagination a, .digg_pagination span {
    padding: .2em .5em;
    display: block;
    float: left;
    margin-right: 1px; }
  .digg_pagination span.disabled {
    color: #999;
    border: 1px solid #DDD; }
  .digg_pagination span.current {
    font-weight: bold;
    background: #ff7700;
    color: white;
    border: 1px solid #999; }
  .digg_pagination a {
    text-decoration: none;
    color: #333;
    border: 1px solid #999; }
    .digg_pagination a:hover, .digg_pagination a:focus {
      color: #ff7700;
      border-color: #003; }
  .digg_pagination .page_info {
    background: #2E6AB1;
    color: white;
    padding: .4em .6em;
    width: 22em;
    margin-bottom: .3em;
    text-align: center; }
    .digg_pagination .page_info b {
      color: #003;
      background: #6aa6ed;
      padding: .1em .25em; }
  .digg_pagination:after {
    content: ".";
    display: block;
    height: 0;
    clear: both;
    visibility: hidden; }
  * html .digg_pagination {
    height: 1%; }
  *:first-child+html .digg_pagination {
    overflow: hidden; }

